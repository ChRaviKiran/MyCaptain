#include<iostream>
using namespace std;
int main()
{
    int i,n;
    int num[10];
    cout<<"Enter size of the array: ";
    cin>>n;//n : size of the array
    cout<<"Enter elements: ";
    for(i=0;i<n;i++)
    {
        cin>>num[i];
    }
    cout<<"You entered: ";
    for(i=0;i<n;i++)
    {
        cout<<num[i]<<"\n";
    }
    return 0;
}
